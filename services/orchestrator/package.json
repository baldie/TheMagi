    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "prettier": "^3.2.5",
    "prettier-plugin-sh": "^0.14.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
