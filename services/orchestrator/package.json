    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,py,sh}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,py,sh}\""
  },
